.rotate{-webkit-transform:rotate(30deg);transform:rotate(30deg);}
.scale{-webkit-transform:scale(2,2);transform:scale(2,2);}
.skew{-webkit-transform:skew(30deg,10deg);transform:skew(30deg,10deg);}
.translate{-webkit-transform:translate(100px,20px);transform:translate(100px,20px);}

/* .matrix{transform:matrix(<number>,<number>,<number>,<number>,<number>,<number>);} */

/* 改变元素基点 */
.origin{-webkit-transform-origin: x-axis y-axis z-axis;transform-origin: x-axis y-axis z-axis;}

/* 过渡用时 */
.ttion{-webkit-transition: all .2s;transition: all .2s!important;
